{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "exceljs": "^4.4.0",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.525.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-barcode": "^1.6.1",
    "react-chartjs-2": "^5.3.0",
    "react-csv": "^2.2.2",
    "react-dom": "^19.1.0",
    "react-grid-layout": "^2.0.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "5.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-rtl": "^0.9.0",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node scripts/build-optimized.js",
    "build:prod": "NODE_ENV=production node scripts/build-optimized.js",
    "build:with-lint": "NODE_OPTIONS='--max-old-space-size=4096' npm run lint:quiet && NODE_OPTIONS='--max-old-space-size=4096' GENERATE_SOURCEMAP=false react-scripts build",
    "build:vps": "NODE_OPTIONS='--max-old-space-size=4096' npm run lint:quiet && NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production react-scripts build",
    "build:fast": "SKIP_PREBUILD=true NODE_OPTIONS='--max-old-space-size=4096' DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:prod:vps": "SKIP_PREBUILD=true NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:instrumented": "node scripts/instrument-build.js && SKIP_PREBUILD=true NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:quiet": "eslint src --ext .js,.jsx,.ts,.tsx --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "clean": "rm -rf build node_modules/.cache",
    "prebuild": "node scripts/prebuild.js",
    "precommit": "npm run lint:fix && npm run format"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.21",
    "jsdom": "^27.2.0",
    "msw": "^2.12.1",
    "postcss": "^8.4.31",
    "vitest": "^4.0.9"
  },
  "proxy": "http://localhost:4000",
  "overrides": {
    "nth-check": "^2.1.1",
    "webpack-dev-server": "^4.15.1",
    "react-scripts": {
      "postcss": "^8.4.31"
    }
  }
}
